{
  "name": "electrontemplate",
  "productName": "electrontemplate",
  "version": "0.0.2",
  "description": "electron template",
  "main": "dist/main.bundle.js",
  "author": {
    "name": "youliso",
    "email": "youliso@Hotmail.com"
  },
  "license": "MIT",
  "scripts": {
    "dev": "node build/script/dev.js",
    "rebuild": "electron-rebuild -f",
    "build:win": "node build/script/build.js && electron-builder -c build/cfg/build.json --win",
    "build:linux": "node build/script/build.js && electron-builder -c build/cfg/build.json --linux",
    "build:mac": "node build/script/build.js && electron-builder -c build/cfg/build.json --mac",
    "postinstall": "electron-builder install-app-deps",
    "test": "jest -c build/cfg/jest.json"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node-fetch": "^2.5.10",
    "@vue/compiler-sfc": "^3.1.1",
    "css-loader": "^5.2.6",
    "electron": "^13.1.2",
    "electron-builder": "^22.11.7",
    "electron-rebuild": "^2.3.5",
    "esbuild-loader": "^2.13.1",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^27.0.4",
    "mini-css-extract-plugin": "^1.5.0",
    "prettier": "^2.3.1",
    "sass": "^1.34.1",
    "sass-loader": "^12.1.0",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.2",
    "vue": "^3.1.1",
    "vue-loader": "^16.2.0",
    "vue-router": "^4.0.9",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "electron-updater": "^4.3.9",
    "node-abort-controller": "^2.0.0",
    "node-fetch": "^2.6.1",
    "socket.io-client": "^4.1.2"
  }
}
